	public static final int GREATEREQUAL = 33;
	public static final int LESS = 34;
	public static final int LESSEQUAL = 35;
	public static final int MUL = 36;
	public static final int DIV = 37;
	public static final int MOD = 38;
	
	public static final int EOF = 39;
	public static final int VOID = 40;
