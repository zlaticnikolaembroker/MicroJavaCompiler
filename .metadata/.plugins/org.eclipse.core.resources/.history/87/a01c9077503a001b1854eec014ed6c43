	public static final int ELSE = 7;
	public static final int DO = 8;
	public static final int WHILE = 9;
	public static final int SWITCH = 10;
	public static final int BREAK = 11;
	public static final int CONTINUE = 12;
	public static final int NEW = 29;
	
	// Identifiers
	public static final int IDENT = 13;
	
	// Constants
	public static final int NUMBER = 14;
	
	// Operators
	public static final int PLUS = 15;
	public static final int EQUAL = 16;
	public static final int COMMA = 17;
	public static final int SEMI = 18;
	public static final int LPAREN = 19;
	public static final int RPAREN = 20;
	public static final int LBRACE = 21;
	public static final int RBRACE = 22;
	public static final int PLUSPLUS = 23;
	public static final int MINUSMINUS = 24;
